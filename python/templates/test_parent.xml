    <hudson.tasks.Shell>
      <command>jiraId=$( cd @jobworkspace ; svn --password &apos; @svnpass &apos; log -q -v --xml --with-all-revprops -r committed | grep msg | sed -e &quot;s/&lt;msg&gt;\([^&lt;]*\)&lt;\/msg&gt;/\1/g&quot; | awk &apos;{print $1;}&apos;;)
url=$BUILD_URL
data=&apos;{
   &quot;update&quot;: {
      &quot;comment&quot;: [
         {
            &quot;add&quot;: {
               &quot;body&quot;:&quot; @projectname - Automation Started.&quot;
            }
         }
      ]
   }
}&apos;
/usr/bin/curl -D- -u user2:user2_pwd# -X PUT -d &quot;$data&quot; -H &quot;Content-Type: application/json&quot; http://jira_host:jira_port/rest/api/2/issue/$jiraId</command>
    </hudson.tasks.Shell>
